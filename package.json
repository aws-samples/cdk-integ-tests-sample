{
  "name": "cdk-integ-tests-sample",
  "version": "0.1.0",
  "bin": {
    "cdk-integ-tests-demo": "bin/cdk-integ-tests-demo.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "watch": "tsc -w",
    "cdk": "cdk",
    "lint": "eslint . && awslint",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "integ-test": "integ-runner --directory ./integ-tests --parallel-regions us-east-1 --update-on-failed"
  },
  "devDependencies": {
    "@aws-cdk/integ-runner": "^2.73.0-alpha.0",
    "@aws-cdk/integ-tests-alpha": "^2.73.0-alpha.0",
    "@types/aws-lambda": "^8.10.111",
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "aws-cdk": "2.73.0",
    "awslint": "^2.51.1",
    "cdk-nag": "^2.25.6",
    "esbuild": "^0.16.7",
    "eslint-plugin-awscdk": "^0.0.11",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "husky": "^8.0.2",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~4.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.278.0",
    "aws-cdk-lib": "2.80.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
