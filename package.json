{
  "name": "cdk-integ-tests-sample",
  "version": "0.1.0",
  "bin": {
    "cdk-integ-tests-demo": "bin/cdk-integ-tests-demo.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "watch": "tsc -w",
    "cdk": "cdk",
    "lint": "eslint . && awslint",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "integ-test": "integ-runner --directory ./integ-tests --parallel-regions us-east-1 --update-on-failed"
  },
  "devDependencies": {
    "@aws-cdk/integ-runner": "^2.88.0-alpha.0",
    "@aws-cdk/integ-tests-alpha": "^2.88.0-alpha.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.3",
    "@types/node": "20.4.2",
    "@types/prettier": "2.7.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "aws-cdk": "2.88.0",
    "awslint": "^2.72.1",
    "cdk-nag": "^2.27.75",
    "esbuild": "^0.18.15",
    "eslint-plugin-awscdk": "^0.0.65",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.996.0",
    "aws-cdk-lib": "2.88.0",
    "constructs": "^10.2.69",
    "source-map-support": "^0.5.21"
  }
}
